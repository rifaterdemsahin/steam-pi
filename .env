# Steam-Pi Environment Variables
# Copy this file to .env.local and fill in values â€” never commit secrets

# Qdrant vector database (for AI search features)
QDRANT_URL=http://localhost:6333
QDRANT_COLLECTION=steam-pi-docs

# Ollama (local LLM)
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=nomic-embed-text
OLLAMA_EMBED_DIMS=4096

# GitHub Pages base URL
SITE_BASE_URL=https://rifaterdemsahin.github.io/steam-pi

# Pico serial port (set at runtime)
PICO_PORT=/dev/cu.usbmodem101
PICO_BAUD=115200
